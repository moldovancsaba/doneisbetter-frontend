{
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains; preload"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "^(.*)$",
      "dest": "/index.html",
      "headers": {
        "x-hello": "world"
      }
    }
  ],
  "trailingSlash": false,
  "cleanUrls": true
}

